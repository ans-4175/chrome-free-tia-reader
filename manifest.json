{
  "manifest_version": 3,
  "name": "Free TiA Reader",
  "description": "Read all TiA Premium Articles",
  "version": "1.0",
  "author": "Anshorimuslim",
  "action": {
    "default_title": "Click to Read TiA Premium",
    "default_icon": "default_icon.png"
  },
  "permissions": [
    "activeTab", "scripting"
  ],
  "host_permissions": [
    "https://freetia-mochfamir.vercel.app/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}